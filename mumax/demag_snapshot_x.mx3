// mumax/demag_snapshot_x.mx3
// Static demag snapshot: uniform m = +x

SetGridSize(128, 128, 1)
SetCellSize(5e-9, 5e-9, 5e-9)

Msat = 8e5
Aex  = 13e-12
Ku1  = 500
anisU = vector(0, 0, 1)

alpha = 0.02

NoDemagSpins.Set(0)
DemagAccuracy = 10

m = uniform(1, 0, 0)

TableAdd(m)
TableAdd(B_demag)
TableAdd(B_eff)
TableAutoSave(1e-12)

// One evaluation, no dynamics
Run(0)